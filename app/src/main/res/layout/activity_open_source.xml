                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Yalantis"
                    android:layout_alignParentEnd="true"
                    android:textSize="18sp"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="20dp">
                <TextView
                    android:id="@+id/tv_name6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="okHttp"
                    android:textSize="18sp"/>

                <TextView
                    android:layout_below="@id/tv_name6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4.7.2"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Square"
                    android:layout_alignParentEnd="true"
                    android:textSize="18sp"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="20dp">
                <TextView
                    android:id="@+id/tv_name7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CircleImageView"
                    android:textSize="18sp"/>

                <TextView
                    android:layout_below="@id/tv_name7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3.1.0"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hdodenhof"
                    android:layout_alignParentEnd="true"
                    android:textSize="18sp"/>
            </RelativeLayout>

        </LinearLayout>


    </ScrollView>



</LinearLayout>